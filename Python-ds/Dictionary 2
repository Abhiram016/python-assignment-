dict={}
n=int(input("enter no of students"))
for i in range(0,n):
	l=[]
	sno=int(input(" enter student number :")) 
	l.append(input("enter student name :"))
	l.append(input("enter student group :"))
	l.append(input(" college :"))
	l.append(input(" enter student number:"))
	dict[sno]=l
sno=int(input("enter student no to view"))
print("student name :"+dict[sno][0])
print("student group :"+dict[sno][1])
print("student college :"+dict[sno][2])
print("student number :"+dict[sno][3])

